{
  "name": "object-key-transformer",
  "version": "0.0.1",
  "description": "object key transformer",
  "main": "dist/amd/index.js",
  "module": "dist/esm/index.js",
  "types": "declarations/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.amd.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "doc": "typedoc -out ./docs ./src --media ./media",
    "test": "npm run test:unit",
    "lint": "eslint \"src/\"",
    "test:unit": "karma start config/karma.config.js --browsers=ChromeHeadless --single-run",
    "test:watch": "karma start config/karma.config.js --browsers=ChromeHeadless --auto-watch",
    "preversion": "npm run test && npm run build && npm run build:esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bichikim/object-key-transformer.git"
  },
  "keywords": [
    "deep",
    "snake-case",
    "camal-case",
    "key",
    "object"
  ],
  "author": "bichi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bichikim/object-key-transformer/issues"
  },
  "homepage": "https://github.com/bichikim/object-key-transformer#readme",
